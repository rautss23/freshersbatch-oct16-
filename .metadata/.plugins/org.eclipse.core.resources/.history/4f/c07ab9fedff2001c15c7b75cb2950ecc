package practice;

public class lamda {

	public static void main(String[] args) {
		Thread myThread = new Thread(new Runnable() {

			@Override
			public void run() {
				System.out.println("hgddscbhbdsh");

			}
		});
		myThread.run();
		
		Thread myLamdaThread = new Thread(()->System.out.println("abcdefg"));
	myLamdaThread.run();
	}

}
