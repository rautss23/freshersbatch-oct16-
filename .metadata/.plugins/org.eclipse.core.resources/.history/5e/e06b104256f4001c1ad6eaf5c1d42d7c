package com.junit;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class MathAddTest {

	@Test
	void test() {
		MathAdd mathAdd=new MathAdd();
		int expected=2;
		int actual=mathAdd.add(1,1);
		assertEquals(expected, actual);
	}
	
	@Test
	void testComputeCircleRadius() {
		MathAdd mathAdd=new MathAdd();
		assertEquals(314.1592653589793, mathAdd.computeCircleArea(10),"Should return area of circle");
	}
	
	@Test
	void tsetDivide(){
		MathAdd mathAdd = new MathAdd();
		assertThrows(NullPointerException.class, ()->mathAdd.divide(1, 0),"divide by zero should throw");
		
	}

}
